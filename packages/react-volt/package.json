{
  "name": "react-volt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vitest run",
    "build": "vite build",
    "build:types": "tsc -b tsconfig.lib.json",
    "build:types:test": "tsc -b tsconfig.test.json",
    "build:api": "pnpm run build:api:lib && pnpm run build:api:test",
    "build:api:lib": "api-extractor run --config api-extractor.json --local --verbose",
    "build:api:test": "api-extractor run --config api-extractor.test.json --local --verbose",
    "clean": "rimraf dist dist-tsc",
    "test": "vitest run"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.0",
    "@rollup/plugin-swc": "^0.3.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-is": "^18.2.4",
    "config": "workspace:*",
    "swc-plugin-de-indent-template-literal": "^1.4.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "1.6.0"
  },
  "files": [
    "dist",
    "*.md"
  ],
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./untrimmed": {
      "types": "./dist/index.untrimmed.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./test": {
      "types": "./dist/test.d.ts",
      "import": "./dist/test.js",
      "require": "./dist/test.js"
    },
    "./package.json": "./package.json"
  }
}
