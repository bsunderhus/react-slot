{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "compiler": {
    "tsconfigFilePath": "<projectFolder>/tsconfig.lib.json"
  },
  "mainEntryPointFilePath": "<projectFolder>/dist-tsc/src/index.d.ts",
  "bundledPackages": [],
  "dtsRollup": {
    "enabled": true,
    "publicTrimmedFilePath": "<projectFolder>/dist/react-sockets.d.ts",
    "omitTrimmingComments": true,
    "untrimmedFilePath": "<projectFolder>/dist/react-sockets.untrimmed.d.ts"
  },
  "docModel": {
    "enabled": false
  },
  "apiReport": {
    "enabled": false
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "error"
      }
    },
    "extractorMessageReporting": {
      "ae-forgotten-export": {
        "logLevel": "none"
      },
      "ae-unresolved-link": {
        "logLevel": "none"
      },
      "default": {
        "logLevel": "warning"
      }
    },
    "tsdocMessageReporting": {
      "tsdoc-escape-greater-than": {
        "logLevel": "none"
      },
      "default": {
        "logLevel": "warning"
      }
    }
  }
}
